{
    "expo": {
        "name": "expo-app",
        "slug": "expo-app",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/logo.png",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "splash": {
            "image": "./assets/images/logo.png",
            "resizeMode": "contain",
            "backgroundColor": "#0F172A"
        },
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.bipulpoudeldev.expo-clerk-social-login"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/logo.png",
                "backgroundColor": "#0F172A"
            }
        },

        "plugins": ["expo-router"],
        "experiments": {
            "typedRoutes": true
        }
    }
}
